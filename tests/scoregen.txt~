

# vbim -newvol 20 20 20 4 float -addnoise 10 5 -thresh 12 -quantize 1 -convert byte -write maskIND.nii.gz z

intact 0 2
damaged 100 2

bounds 0 100

lesions lesions.nii.gz
voxel v1 1 1 1
